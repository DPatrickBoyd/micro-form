{
  "name": "micro-form",
  "version": "1.4.1",
  "description": "Low-level localized state form building library for React.",
  "main": "dist/index.js",
  "license": "MIT",
  "repository": "https://github.com/estrattonbailey/micro-form",
  "author": "estrattonbailey",
  "scripts": {
    "lint": "standard 'index.js'",
    "prebuild": "npm run lint",
    "build": "NODE_ENV=production babel index.js -d dist/",
    "watch": "NODE_ENV=development babel -w --no-comments index.js -d dist/",
    "prepublish": "npm run build",
    "copy": "rm README.md ; cp ../README.md ./"
  },
  "keywords": [
    "react",
    "form",
    "forms"
  ],
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.21.0",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-babili": "0.0.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.23.0",
    "standard": "^9.0.2"
  },
  "peerDependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "dependencies": {
    "microstate": "^0.1.3",
    "prop-types": "^15.5.10"
  }
}
